{
  "name": "Zatcher",
  "version": "1.0.0",
  "summary": "A simple logger",
  "description": "A simple logger",
  "homepage": "https://github.com/lzackx/Zatcher",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lzackx": "lzackx@lzackx.com"
  },
  "source": {
    "git": "https://github.com/lzackx/Zatcher.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "static_framework": true,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Zatcher/Classes/*.[h,m]",
        "Zatcher/Classes/RenderMonitor/**/*.[h,m]"
      ],
      "dependencies": {
        "PLCrashReporter": [
          "~> 1.8"
        ]
      }
    },
    {
      "name": "OrderGenerator",
      "source_files": "Zatcher/Classes/OrderGenerator/**/*.[h,m]"
    }
  ]
}
